#include<stdio.h>
int main()
{
	int a[3][3] , b[3][3] , i , j , c[3][3];
	printf("Enter 9 elements of 1st matrix\n");
	for(i=0;i<=2;i++)
	for(j=0;j<=2;j++)
		scanf("%d",&a[i][j]);
	printf("Enter 9 elements of 2nd matrix\n");
	for(i=0;i<=2;i++)
	for(j=0;j<=2;j++)
		scanf("%d",&b[i][j]);
		printf("Required matrix is = \n");
	for(i=0;i<=2;i++)
{
	for(j=0;j<=2;j++)
	{
		c[i][j]=a[i][j] + b[i][j];
		printf(" %d",c[i][j]);
	}
	printf("\n");
}

return 0;
}


